<Model:BaseWindow x:Class="Browser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:Misc="clr-namespace:Browser.Misc"
                 xmlns:Model="clr-namespace:Browser.Model" Title="WMI Browser" Height="600" Width="800">
    <Grid>
		<TabControl Height="600" HorizontalAlignment="Left"  Name="tabs" VerticalAlignment="Top" Width="800">
			<TabItem Header="Browse" Name="BrowserTab">
				<StackPanel Height="600" Width="800" Name="BrowseTabPanel" />
			</TabItem>
			<TabItem Header="Custom Query" Name="CustomQueryTab">
				<StackPanel Height="600" Name="customQueryTabPanel" Width="800" />
			</TabItem>
			<TabItem Header="Demo" Name="DemoTab">
				<StackPanel Height="600" Name="demoTabPanel" Width="800" />
			</TabItem>
		</TabControl>
	</Grid>
</Model:BaseWindow>
